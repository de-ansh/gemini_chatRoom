{
  "name": "gemini-chatroom-backend",
  "version": "1.0.0",
  "description": "Gemini-style backend system with chatrooms, OTP authentication, and Stripe integration",
  "main": "dist/index.js",
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/controllers": "dist/controllers",
    "@/middleware": "dist/middleware",
    "@/services": "dist/services",
    "@/models": "dist/models",
    "@/utils": "dist/utils",
    "@/types": "dist/types",
    "@/routes": "dist/routes",
    "@/queues": "dist/queues"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "backend",
    "express",
    "typescript",
    "gemini",
    "chatroom",
    "stripe"
  ],
  "author": "Backend Developer",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "prisma": "^5.7.1",
    "redis": "^4.6.12",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
